Object subclass: #CuOOra	instanceVariableNames: 'usuarios topicos'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!!CuOOra methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 00:33'!obtenerPreguntasRelevantesDe: unUsuario	^ self preguntasDeLosUsuarios		select: [ :each | 			(unUsuario teInteresaEstaPregunta: each)				or: (each tuCreadorEsSeguidoPor: unUsuario) ]! !!CuOOra methodsFor: 'accesing' stamp: 'Anonymous 3/31/2021 21:12'!usuarios	^ usuarios! !!CuOOra methodsFor: 'accesing' stamp: 'Anonymous 3/31/2021 21:13'!topicos	^ topicos ! !!CuOOra methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 01:00'!obtenerPreguntasRelacionadasA: unTopico	^ self preguntasDeLosUsuarios		select: [ :each | each tenesComoTopicoA: unTopico ]! !!CuOOra methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 00:15'!preguntasDeLosUsuarios	^ usuarios flatCollect: [ :each | each misPreguntas ]! !!CuOOra methodsFor: 'initialization' stamp: 'Anonymous 3/31/2021 21:15'!initialize	usuarios := OrderedCollection new.	topicos := OrderedCollection new. ! !Object subclass: #Posteo	instanceVariableNames: 'creadorDelPosteo fechaYHoraDeCreacion puntajes'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!!Posteo methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 00:32'!tuCreadorEsSeguidoPor: unUsuario 	^ (creadorDelPosteo teSigue: unUsuario )! !Posteo subclass: #Pregunta	instanceVariableNames: 'titulo descripcion topicos respuestas'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!!Pregunta methodsFor: 'initialization' stamp: 'Anonymous 3/26/2021 23:05'!initialize 	fechaYHoraDeCreacion := Date today.	respuestas := OrderedCollection new.	! !!Pregunta methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:04'!newPreguntaConTitulo: unTitulo descripcion: unaDescripcion topico: unTopico usuario: unUsuario	titulo := unTitulo.	descripcion := unaDescripcion.	topicos add: unTopico.	creadorDelPosteo := unUsuario! !!Pregunta methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:30'!tenesComoTopicoA: unTopico	^ topicos anySatisfy: [ :each | each nombre ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pregunta class	instanceVariableNames: ''!!Pregunta class methodsFor: 'creation' stamp: 'Anonymous 3/26/2021 23:09'!newPreguntaConTitulo: unTitulo descripcion: unaDescripcion topico: unTopico usuario: unUsuario	^ self new		newPreguntaConTitulo: unTitulo		descripcion: unaDescripcion		topico: unTopico		usuario: unUsuario! !Object subclass: #Puntaje	instanceVariableNames: 'puntuador esLike'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!Posteo subclass: #Respuesta	instanceVariableNames: 'texto'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!Object subclass: #Topico	instanceVariableNames: 'nombre descipcion'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!!Topico methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:30'!nombre	^ nombre ! !Object subclass: #Usuario	instanceVariableNames: 'topicosDeInteres follows followers preguntas nombreDeUsuario password'	classVariableNames: ''	package: 'Objetos2-CuOORa-Model'!!Usuario methodsFor: 'initialization' stamp: 'Anonymous 3/26/2021 23:26'!initialize	followers := OrderedCollection new.	follows := OrderedCollection new.	preguntas := OrderedCollection new.	topicosDeInteres := OrderedCollection new! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:14'!agregarPregunta: unaPregunta	preguntas add: unaPregunta! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 00:21'!teInteresaEstaPregunta: unaPregunta	^ topicosDeInteres		anySatisfy: [ :each | unaPregunta tenesComoTopicoA: each ]! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 00:31'!teSigue: unUsuario	^ followers		anySatisfy: [ :each | each nombreDeUsuario = unUsuario nombreDeUsuario ]! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/27/2021 00:27'!nombreDeUsuario	^ nombreDeUsuario ! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:20'!seguirA: unUsuario	follows add: unUsuario.	unUsuario agregarSeguidor: self! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:26'!misPreguntas	^ preguntas ! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/31/2021 21:06'!nombreDeUsuario: unNombre password: pass	nombreDeUsuario := unNombre.	password := pass! !!Usuario methodsFor: 'accesing' stamp: 'Anonymous 3/26/2021 23:20'!agregarSeguidor: unUsuario	followers add: unUsuario! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Usuario class	instanceVariableNames: ''!!Usuario class methodsFor: 'creation' stamp: 'Anonymous 3/31/2021 21:07'!nombreDeUsuario: unNombre password: pass	^ self new nombreDeUsuario: unNombre password: pass! !